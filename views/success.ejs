<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Survey Form Success</title>
</head>
<body>
    <div class="container">
        <h1>On success page</h1>
        <table border='1'>
            <tr>
                <th>Name:</th>
                <th>Location:</th>
                <th>Language:</th>
                <th>Comment:</th>
            </tr>
            <% for(let user in users) { %>
                <tr>
                    <td><%= users[user].name %></td>
                    <td><%= users[user].location %></td>
                    <td><%= users[user].language %></td>
                    <td><%= users[user].comment %></td>
                </tr>
            <% } %>
        </table>
        <a href="/">Back....</a>
    </div>
</body>
</html>